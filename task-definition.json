{
  "family": "s4-final-task",
  "containerDefinitions": [
    {
      "name": "springboot",
      "image": "022229768579.dkr.ecr.us-east-1.amazonaws.com/s4-final-backend-springboot:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://postgres:5432/misight_db"
        },
        {
          "name": "SPRING_DATASOURCE_USERNAME",
          "value": "postgres"
        },
        {
          "name": "SPRING_DATASOURCE_PASSWORD",
          "value": "admin"
        }
      ]
    },
    {
      "name": "postgres",
      "image": "022229768579.dkr.ecr.us-east-1.amazonaws.com/s4-final-backend-postgres:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5432,
          "hostPort": 5432,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "POSTGRES_DB",
          "value": "misight_db"
        },
        {
          "name": "POSTGRES_USER",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "admin"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "memory": "1GB",
  "cpu": "0.5 vCPU",
  "executionRoleArn": "arn:aws:iam::022229768579:role/ecsTaskExecutionRole"
}
